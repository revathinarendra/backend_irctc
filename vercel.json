{
    "version": 2,
    "builds": [
      {
        "src": "booking/wsgi.py",
        "use": "@vercel/python",
        "config": { 
          "runtime": "python3.9",
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "booking/wsgi.py"
      }
    ]
  }
  